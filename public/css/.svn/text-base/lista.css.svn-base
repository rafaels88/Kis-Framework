ol.lista {
    list-style-type: none;
}
ol.lista li {
    margin: 10px 0;
}
ol.lista li a {
    color: #AF270A;
    font-family: verdana;
    font-size: medium;
}
ol.lista li a:hover {
    text-decoration: underline;
}
ol.lista li div.descricao {
    font-size: small;
    font-family: arial, sans-serif;;
    margin-left: 20px;
}
